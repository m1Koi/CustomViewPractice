<?xml version="1.0" encoding="utf-8"?>
<com.m1Ku.progressview.view.view6.KGSlidingMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/rect_color"
    app:rightMenuMargin="50dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <include
            layout="@layout/layout_sliding_menu"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <include
            layout="@layout/layout_sliding_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

</com.m1Ku.progressview.view.view6.KGSlidingMenu>